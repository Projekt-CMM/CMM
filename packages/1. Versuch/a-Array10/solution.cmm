#include <stdio.h>
#include <stdlib.h>
#include <array.h>

void main(){
	
	//Das Array
	int array[100], x, y;

	//Array initialisieren
	for( x = 0; x < 100; x++ ) {
		array[x] = x*10;
	}

	//Ausgabe
	for( y = 0; y < 10; y++ ) {
		for( x = 0; x < 10; x++ ) {
			printf("%d ", array[x+10*y]);
		}
		printf("\n");
	}
}